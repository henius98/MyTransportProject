{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "AppSettings": {
        "PollingIntervalMinutes": 1,
        "MaxRetryAttempts": 1,
        "RetryDelaySeconds": 5,
        "EnableHealthMonitoring": true,
        "SaveToDatabase": false,
        "OutputDirectory": "./GTFSData"
    }
}
